#!/usr/bin/env ruby
# frozen_string_literal: true

# This script is used both in CI and in local development 'rspec' runs.

require_relative '../spec/support/helpers/gitaly_setup'

class GitalyTestSpawn
  include GitalySetup

  def run
    install_gitaly_gems
    spawn_gitaly
  end
end

GitalyTestSpawn.new.run
