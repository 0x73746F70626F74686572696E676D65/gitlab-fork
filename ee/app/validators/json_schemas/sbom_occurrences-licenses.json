{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "List of licenses associated with an SBOM occurrence",
  "type": "array",
  "items": [
    {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "minLength": 1,
          "maxLength": 50
        },
        "url": {
          "type": "string",
          "format": "uri"
        }
      },
      "required": [
        "name",
        "url"
      ],
      "additionalProperties": false
    }
  ]
}
