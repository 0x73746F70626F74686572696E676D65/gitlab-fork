.container-fluid.container-limited{ class: "gl-pb-2! gl-pt-6! #{content_class}" }
  = render Pajamas::AlertComponent.new(alert_class: 'js-user-over-limit-free-plan-alert',
        alert_data: alert_data,
        title: alert_attributes[:title],
        close_button_data: close_button_data) do |c|
    = c.body do
      = alert_attributes[:body]
    = c.actions do
      = alert_attributes[:primary_cta]
      = alert_attributes[:secondary_cta]
